      <name>CFS</name>
      <description>Custom functions subsystem</description>
      <baseAddress>0xFFEB0000</baseAddress>

      <interrupt><name>CFS_FIRQ</name><value>1</value></interrupt>

      <addressBlock>
        <offset>0</offset>
        <size>0x10000</size>
        <usage>registers</usage>
